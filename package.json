    "doc": "doc"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix \"app/**/*.js\"",
      "prettier --write --no-semi --single-quote --trailing-comma=es5",
      "git add"
    ]
  },
  "pre-commit": {
    "run": [
      "lint:staged"
    ],
    "silent": true
  },
  "scripts": {
